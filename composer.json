{
    "version": "1.0.4",
    "name": "insitaction/easycrop",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Insitaction\\EasyCropBundle\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Blairy Julien",
            "email": "jblairy@insitaction.com"
        }
    ],
    "require": {
        "symfony/http-kernel": "^6.3|^6.2|^6.1|^6.0",
        "symfony/dependency-injection": "^6.3|^6.2|^6.1|^6.0",
        "symfony/config": "^6.3|^6.2|^6.1|^6.0",
        "symfony/webpack-encore-bundle": "^1.0|^2.0",
        "easycorp/easyadmin-bundle": "^4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.22",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/extension-installer": "^1.3"
    },
    "scripts": {
        "phpcsfixer": "./vendor/bin/php-cs-fixer fix",
        "phpcsfixer-lint": "./vendor/bin/php-cs-fixer fix --dry-run --diff",
        "phpstan": "./vendor/bin/phpstan analyse --no-progress",
        "ci": [
            "@phpcsfixer-lint",
            "@phpstan"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
